{
  "parser": "babel-eslint",
  "plugins": [
    "flowtype"
  ],
  "extends": "watsi",
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "max-len": "off",
    "no-bitwise": "off",
    "no-multi-spaces": "off",
    "no-return-assign": ["error", "except-parens"],
    "react/sort-comp": "off"
  }
}
